"use strict";

(function() {
    const FLAGS = {};

    const SWITCHES = {
        'help_button': true,
        'monthly_report_notification': true,
        'delete_account_oauth': true,
        'good_news': true,
        'summary_comparison': true,
        'advanced_filters': true,
        'business_hours': true,
        'extra_advanced_filters': true,
        'referral_program': true,
        'subject_automated_filters': true,
        'top_navbar_active': true,
        'delegated_inboxes': true,
        'referral_widget':true,
        'show_nolt': true,
        'yearly_report_2020': true,
        'alerting': true
    };

    const SAMPLES = {

    };

    window.waffle = {
        "flag_is_active": function waffle_flag(flag_name) {
            return !!FLAGS[flag_name];
        },
        "switch_is_active": function waffle_switch(switch_name) {
            return !!SWITCHES[switch_name];
        },
        "sample_is_active": function waffle_sample(sample_name) {
            return !!SAMPLES[sample_name];
        },
        "FLAGS": FLAGS,
        "SWITCHES": SWITCHES,
        "SAMPLES": SAMPLES
    };
})();
